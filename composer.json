{
    "name": "innmind/genome",
    "type": "library",
    "description": "Tool to deploy projects and libraries on a machine",
    "keywords": ["deploy", "project", "library"],
    "homepage": "http://github.com/Innmind/Genome",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Genome/issues"
    },
    "require": {
        "php": "~7.4",
        "innmind/operating-system": "^2.1",
        "innmind/cli-framework": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Genome\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Genome\\": "tests/",
            "Fixtures\\Innmind\\Genome\\": "fixtures/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.4",
        "innmind/black-box": "^4.9",
        "nikic/php-parser": "~4.10",
        "innmind/coding-standard": "^1.1"
    },
    "bin": ["genome"]
}
