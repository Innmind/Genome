{
    "name": "innmind/genome",
    "type": "library",
    "description": "Tool to deploy projects and libraries on a machine",
    "keywords": ["deploy", "project", "library"],
    "homepage": "http://github.com/Innmind/Genome",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Genome/issues"
    },
    "require": {
        "php": "~7.4",
        "innmind/immutable": "^2.8",
        "innmind/url": "^2.0",
        "innmind/server-control": "^2.3",
        "innmind/cli": "^1.4",
        "innmind/filesystem": "^3.1",
        "innmind/json": "^1.1",
        "composer/semver": "^1.5",
        "innmind/http-transport": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Genome\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Genome\\": "tests/",
            "Fixture\\Innmind\\Genome\\": "fixtures/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "giorgiosironi/eris": "^0.11.0"
    },
    "bin": ["genome"]
}
